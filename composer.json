{
    "name": "easybib/silex-qafoo-provider",
    "description": "Silex provider for the qafoo profiler",
    "keywords": [
        "profiler", "logging", "performance", "qafoo", "tideways"
    ],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Stephan Vock",
            "email": "stephan@lagged.biz"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "silex/silex": "^2.0"
    },
    "autoload": {
        "psr-4": {"EasyBib\\Silex\\": "src/"}
    },
    "suggest": {
        "doctrine/dbal": "If you'd like to use the fancy logger to profile queries.",
        "ext-tideways": "You should install ext/tideways: https://github.com/tideways/php-profiler-extension"
    }
}
